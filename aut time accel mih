local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

	local sussy = 0
	local plr = game.Players.LocalPlayer
	local char = plr.Character
	
	local function thing()
	    char.Humanoid.WalkSpeed = 69
		for i,v in pairs(char:GetChildren()) do
			if v:IsA("Part") and v.Name ~= "Head" then
				local blur1 = game.ReplicatedStorage["Client Assets"]["Made In Heaven"].Blur.blur:Clone()
				local blur2 = game.ReplicatedStorage["Client Assets"]["Made In Heaven"].Blur.blur:Clone()
				blur1.Parent = v
				blur2.Parent = v
				spawn(function()
					repeat
						wait()
					until sussy>=250
					blur1:Destroy()
					blur2:Destroy()
				end)
				if v.Name ~= "Head" or v.Name ~= "Torso" then
					local trail = game.ReplicatedStorage["Client Assets"]["Made In Heaven"].Blur.Speed:Clone()
					trail.Parent = workspace.Effects
					local weld = Instance.new("Weld",trail)
					weld.Part0 = v
					weld.Part1 = trail
					spawn(function()
						repeat
							wait()
						until sussy>=250
						trail:Destroy()
					end)
				end
			end
		end
		local ae = false
		repeat
			sussy = sussy + 1
			spawn(function()
				repeat wait() until ae == false
				ae = true
				for i = 1,25 do
					game.Lighting:SetMinutesAfterMidnight(game.Lighting:GetMinutesAfterMidnight()+1)
					wait()				
					if sussy >= 250 then
						break
					end
				end 
				ae = false				
			end)
			spawn(function()
				for i = 1,5 do
					local clone = game.ReplicatedStorage["Client Assets"]["Made In Heaven"].dummy:Clone()
					clone.Parent = workspace.Effects
					for i,v in pairs(clone:GetChildren()) do
						v.CFrame = char[v.Name].CFrame
						v.CanCollide = false
						game.TweenService:Create(v,TweenInfo.new(.1),{Transparency = 1}):Play()
					end
					game.Debris:AddItem(clone,.1)
					wait()
				end				
			end)
			wait(.05)
		until sussy >= 250
		sussy = 0
		char.Humanoid.WalkSpeed = 16
	end
	
local player = game.Players.LocalPlayer
local UIS = game:GetService("UserInputService")
		
UIS.InputBegan:connect(function(input,gameProcessedEvent)
	if gameProcessedEvent then return end --If player is typing return
	if input.UserInputType == Enum.UserInputType.Keyboard then --If player is using a KeyBoard
		if input.KeyCode  == Enum.KeyCode.K then -- Connects when the key is touched
		    if not player.Data.Ability == 27 or player.Data.Ability == 28 then return end
		    thing()
	   	end
	end
end)
